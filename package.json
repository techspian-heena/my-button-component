  "description": "Orxe Component",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "license": "UNLICENSED",
  "private": false,
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "ORXe",
  "scripts": {
    "test": "jest",
    "test:skipaxe": "jest \"^(.*/)*(.*\\.)*(spec)\\.ts$\"",
